<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="BitMash" file="BitMash" main="Main" version="0.0.1" company="STVR" />
	
	<window width="800" height="400" fps="30" orientation="portrait" background="#ffffff" resizable="true" if="web" />
	<window width="800" height="400" fps="30" orientation="landscape" background="#000000" fullscreen="false" hardware="true" vsync="true" unless="web" />
	
	<set name="SWF_VERSION" value="11.4" />
	<set name="BUILD_DIR" value="export" />
	
	<classpath name="source" />
	
	<haxelib name="openfl" />
	<haxelib name="format" />
	
	<icon path="assets/bitmash.svg" />
	
	<app preloader="MashPreloader" unless="desktop" />
</project>